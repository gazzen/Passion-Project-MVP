
@model FoodOrderMgmtApplication.Models.customerDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this cutomer?</h2>
<a href="/Customer/List">Cancel</a>
<h2>@Model.CustomerFirstName</h2>
<div>Address: @Model.CustomerAddress</div>
<div>EmailID: @Model.CustomerEmailId</div>
<div>Contact: @Model.CustomerPhone</div>
<form action="/Customer/Delete/@Model.CustomerId" method="POST">
    <input type="submit" value="Confirm Delete" />

</form>